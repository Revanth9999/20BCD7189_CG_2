def setup():
    size(600, 600);

def draw():
    background(220);
    # To draw house outline
    line(100, 100, 150, 50);
    line(150, 50, 200, 100);
    line(150, 50, 350, 50);
    line(350, 50, 400, 100);
    rect(100, 100, 100, 100);
    rect(200, 100, 200, 100);
    # To draw door and windows
    rect(130, 120, 40, 80);
    rect(240, 130, 40, 40);
    rect(320, 130, 40, 40);
    # To draw the road
    rect(0, 280, 600, 100);
    line(50, 325, 100, 325);
    line(150, 325, 200, 325);
    line(250, 325, 300, 325);
    line(350, 325, 400, 325);
    line(450, 325, 500, 325);
